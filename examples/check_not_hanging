echo 'check_storage_drive_predictive_failure{array="0", controller="0", drive="0", product="SYS-2028TP-HC1TR"} 0'
echo 'check_storage_drive_errors{array="0", controller="0", drive="1", product="SYS-2028TP-HC1TR"} 0'
